# Enigma API Configuration
# Production server (default)
ENIGMA_SERVER=api.enigmaai.net:443
# Staging server (uncomment to use)
#ENIGMA_SERVER=dev.getenigma.ai:443
# Local development (uncomment to use)
#ENIGMA_SERVER=localhost:8080

# Your API key (required)
ENIGMA_API_KEY=your_api_key_here

# Capture Configuration
CAPTURE_INTERFACE=eth0  # Linux: eth0, ens33, etc. | Windows: Ethernet, Wi-Fi, etc.
CAPTURE_DURATION=60s    # Duration format: 60s, 5m, 1h, etc.
CAPTURE_OUTPUT_DIR=./captures

# TLS Configuration
# Set to true for local development, false for production/staging
DISABLE_TLS=false